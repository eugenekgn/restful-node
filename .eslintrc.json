{
  "extends": "airbnb",
  "plugins": [
    "react",
    "jsx-a11y",
    "import",
    "mocha",
    "chai-expect"
  ],
  "rules": {
    "object-curly-spacing": 0,
    "class-methods-use-this": 0,
    "strict": 0,
    "consistent-return": 0,
    "global-require": 0,
    "no-else-return": 0,
    "arrow-body-style": 0,
    "comma-dangle": 0,
    "no-trailing-spaces": 0,
    "spaced-comment": 0,
    "import/no-extraneous-dependencies": 0,
    "mocha/no-exclusive-tests": "error",
    "no-underscore-dangle": 0
  },
  "env": {
    "node": true,
    "mocha": true
  }
}
